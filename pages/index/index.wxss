/**index.wxss**/
.hidden {
	visibility: hidden;
}

.btn {
	width: 90%;
	margin: 0 auto 30rpx;
}

.btn[disabled],.btn[disabled]:not([type]) {
	color: #000;
}

.voicePlay {
	animation-name: voicePlay;
	animation-duration: 1s;
	animation-direction: normal;
	animation-iteration-count: infinite;
	animation-timing-function: steps(3);
}

@keyframes voicePlay {
	0% {
		background-position: 0;
	}

	100% {
		background-position: 100%;
	}
}

.wrapper {
	position: absolute;
	width: 100%;
	height: 100%;
}

.tip {
	color: #AAA;
	font-size: 30rpx;
}

#examSec {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}

#examSec > view:nth-child(1) {
	width: 90%;
	margin: 15rpx auto 0;
}

#hitSec {
	position: absolute;
	right: 5%;
	top: 8%;
}

#hitSec view {
	border: 1px solid #ccc;
	border-radius: 5px;
	display: inline-block;
	width: 100rpx;
	height: 52rpx;
	line-height: 52rpx;
}

#hitSec view:first-child:not(:last-child) {
	border-top-right-radius: 0;
	border-bottom-right-radius: 0;
}

#hitSec view:not(:first-child) {
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}

#hitSec view + view {
	margin-left: -1px;
}

#text_hint {
	background: url(data:image/png;base64,) no-repeat center center / 22rpx 26rpx;
}

#audio_hint text {
	width: 29rpx;
	height: 100%;
	display: block;
	margin: 0 auto;
	background: url(data:image/png;base64,) no-repeat right center / 400%;
}

#questionSec {
	height: 300rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	margin-bottom: 100rpx;
}

#question {
	font-size: 130rpx;
	line-height: 1.2;
}

#question_romaji {
	font-size: 50rpx;
	color: #00acc2;
}

#question_analyze {
	color: #F00;
	font-size: 30rpx;
	margin-top: 15rpx;
}

#optionsSec {
	width: 100%;
}

#resultSec {
	width: 90%;
	margin: 0 auto;
}

#resultSec > view:nth-child(0) {
	padding-top: 30rpx;
}

#rate {
	font-size: 130rpx;
	color: #00acc2;
	text-align: center;
	padding: 100rpx 0;
}

#resultSec view:last-child {
	text-align: center;
	font-size: 35rpx;
}

.correct {
	background: #F8F8F8 url(data:image/png;base64,) no-repeat 95% 50% / 75rpx 52rpx;
}

.error {
	background: #F8F8F8 url(data:image/png;base64,)  no-repeat 95% 50% / 50rpx;
} 